<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="TestNG Parameters Concept" thread-count="3" parallel="tests" verbose="4">
	<test name="Flipkart Test">
		<parameter name="url" value="https://www.flipkart.com/" />
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="com.testng.parametersconcept.FlipkartTest" />
		</classes>
	</test>
	<test name="Google Test">
		<parameter name="url" value="https://www.google.com/" />
		<parameter name="browser" value="edge" />
		<classes>
			<class name="com.testng.parametersconcept.GoogleTest" />
		</classes>
	</test>
	<test name="Snapdeal Test">
		<parameter name="url" value="https://www.snapdeal.com/" />
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="com.testng.parametersconcept.SnapDealTest" />
		</classes>
	</test>
</suite>